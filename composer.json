{
    "name": "tenolo/role-bundle",
    "description": "Implements role functions.",
    "keywords": [
        "role",
        "tenolo",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",

        "tenolo/core-bundle": "~2.0",
        "tenolo/entity-bundle": "~2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/repo/private/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Bundle\\RoleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Bundle\\RoleBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/2.0": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
